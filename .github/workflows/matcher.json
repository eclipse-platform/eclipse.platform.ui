{
  "problemMatcher": [
    {
      "owner": "api-tools",
      "pattern": [
        {
          "regexp": "^\\* Marker \\[on:\\s+([^,]+).*charStart:\\s*(\\d+).*lineNumber:\\s*(\\d+).*message:\\s* ([^,]+).*severity:\\s2.*sourceId:\\sAPI Tools.*\\]$",
          "file": 1,
          "column": 2,
          "line": 3,
          "message": 4
        }
      ]
    }
  ]
}

