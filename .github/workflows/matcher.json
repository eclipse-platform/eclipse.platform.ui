{
  "problemMatcher": [
    {
      "owner": "api-tools",
      "pattern": [
        {
          "regexp": "^.*\\/([^/]+\\.java),.*lineNumber:\\s*(\\d+).*message:\\s*(.*),\\s*messagearguments.*severity:\\s2.*sourceId:\\sAPI Tools.*\\]$",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    }
  ]
}

