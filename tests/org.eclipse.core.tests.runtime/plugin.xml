<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
  name="Eclipse Core Tests Runtime"
  id="org.eclipse.core.tests.runtime"
  version="3.0"
  provider-name="Eclipse.org">

  <requires>
	<import plugin="org.eclipse.osgi.services"/>
	<import plugin="org.eclipse.core.runtime.compatibility"/> 
    <import plugin="org.eclipse.core.resources"/>
    <import plugin="org.eclipse.core.tests.harness"/>
    <import plugin="org.junit"/>
  </requires>

  <runtime>
    <library name="runtimetests.jar">
      <export name="*"/>
    </library>
  </runtime>

	<extension id="platform-preferences" point="org.eclipse.core.runtime.preferences" name="platform-preferences">
		<preferences scope="test" class="org.eclipse.core.tests.internal.preferences.TestScope"/>
	</extension>

	<extension point="org.eclipse.core.runtime.urlHandlers">
		<handler
			class="org.eclipse.core.tests.runtime.stream.Handler"
			protocol="stream">
		</handler>
	</extension>
	
	<extension point="org.eclipse.core.runtime.contentTypes">
		<content-type 
			id="fooBar" 
			name="Foo Bar" 
			file-extensions="bar"
			file-names="foo.bar"/>	
		<content-type 
			id="xml-based-different-extension" 
			name="XML Based with Different Extension" 
			base-type="org.eclipse.core.runtime.xml" 
			charset="UTF-16LE"
			file-extensions="xml2"/>
		<content-type 
			id="xml-based-specific-name" 
			name="XML Based with Specific Name" 
			base-type="org.eclipse.core.runtime.xml"
			file-names="xml-based.xml"/>		
		<content-type 
			id="sample-binary"
			name="Sample Binary"
			file-extensions="samplebin"/>
		<content-type 
			id="orphan" 
			name="Orphan Type" 
			base-type="org.eclipse.foo.bar"
			file-extensions="orphan"
			file-names="foo.orphan"/>						
	</extension>	

	<extension point="org.eclipse.core.runtime.adapters">
		<factory class="org.eclipse.core.tests.runtime.TestAdapterFactory"
			adaptableType="org.eclipse.core.tests.runtime.TestAdaptable">
			<adapter type="org.eclipse.core.tests.runtime.TestAdapter"/>
		</factory>
	</extension>

